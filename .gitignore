# Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep
!/tmp/pids/.keep

# Ignore uploaded files in development
/storage/*
!/storage/.keep

/node_modules
/yarn-error.log

/public/assets
.byebug_history

# Ignore master key for decrypting credentials and more.
/config/master.key

#Ignore modifications of config/database.yml
/config/database.yml

#Ignore modifications for .env
/.env

#Ignore Changes in uploads
public/uploads/

/public/packs
/public/packs-test
/node_modules
/yarn-error.log
yarn-debug.log*
.yarn-integrity
.nvmrc

!/log/development.log
!/tmp/cache/bootsnap/load-path-cache

log/development.log
tmp/cache/assets/sprockets/
tmp/cache/bootsnap/load-path-cache
tmp/cache/assets/sprockets/v4.0.0/bp/